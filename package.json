{
	"name": "@grist/ira-tracker",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"packageManager": "pnpm@9.15.4",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"gen:geojson": "tsx scripts/csv-to-geojson.ts",
		"gen:pmtiles": "tsx scripts/geojson-to-pmtiles.ts",
		"upload:geojson": "tsx scripts/upload-geojson.ts",
		"upload:pmtiles": "tsx scripts/upload-pmtiles.ts",
		"upload:styles": "tsx scripts/upload-styles.ts",
		"process-all": "pnpm gen:geojson && pnpm gen:pmtiles && pnpm upload:geojson && pnpm upload:pmtiles && pnpm upload:styles",
		"publish:app": "tsx scripts/deploy.ts",
		"build-and-publish": "pnpm build && pnpm publish:app"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/node": "^22.13.1",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"svelte": "^5.19.1",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0",
		"vite": "^6.0.11"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.741.0",
		"@turf/turf": "^7.2.0",
		"@types/geojson": "^7946.0.16",
		"@types/kdbush": "^3.0.5",
		"@types/lodash-es": "^4.17.12",
		"csv-parse": "^5.6.0",
		"d3-scale-chromatic": "^3.1.0",
		"kdbush": "^4.0.2",
		"lodash-es": "^4.17.21",
		"maplibre-gl": "^5.0.1",
		"pmtiles": "^4.2.1",
		"roughjs": "^4.6.6",
		"tippecanoe": "^0.6.0"
	}
}
